     <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <title>Document</title>
     <link rel="stylesheet" href="/stylesheet/style.css">
     <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
     <!-- <h1>Hey! Jitin K Gupta ðŸ‘Œ</h1> -->
     <div class="main w-full  min-h-screen bg-zinc-900 p-10 text-white">
          <a href="/" class="text-blue-500 inline-block mb-10">Go Back to Home</a>
          <form action="/edit" method="post">
               <input type="text" class="block w-full rounded-lg px-3 py-2 bg-zinc-800" name="previous" id="" placeholder="Previous" value="<%= filename %>" required>
               <input type="text" class="block mt-2 w-full rounded-lg px-3 py-2 bg-zinc-800" name="new" id="" placeholder="New name goes here.." required>
               <input class="mt-2 bg-yellow-600 px-5 py-2 rounded-md" type="submit" value="Update name">
          </form>
        
     </div>
     
</body>
